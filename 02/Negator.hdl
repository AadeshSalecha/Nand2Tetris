/**
 * Negator:
 * 		negates a 16-bit number
 */

CHIP Negator {
    IN in[16];
    OUT out[16];

    PARTS:
   // Put you code here:
   Not16(in = in, out = nota);
   Inc16(in = nota, out = out);  
}